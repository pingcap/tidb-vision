{
  "name": "pd-vis",
  "description": "tidb pd visualiation",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --env.dev --host 0.0.0.0",
    "mock": "nodemon --watch mock/server.js mock/server.js",
    "start": "npm-run-all -p mock dev",
    "build": "rimraf dist && webpack --progress --hide-modules",
    "preview": "static dist",
    "ghpages": "git add -f dist && git commit -m 'publish';git subtree push --prefix dist origin gh-pages"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "clipboard-js": "^0.3.6",
    "d3": "^4.12.0",
    "d3-hierarchy": "^1.1.5",
    "d3-queue": "^3.0.7",
    "d3-scale-chromatic": "^1.1.1",
    "d3-svg-annotation": "^2.1.0",
    "d3-svg-legend": "^2.25.1",
    "lodash": "^4.17.4",
    "vue": "^2.5.2"
  },
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-vue-app": "^1.2.0",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.27.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.24.1",
    "lodash.merge": "^4.6.0",
    "nodemon": "^1.12.5",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^0.5.8",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5",
    "jsxBracketSameLine": true
  }
}
